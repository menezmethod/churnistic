{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true,
      "preview": true
    }
  },
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 